@ARTICLE{Gibelin2006,
  author = {Gibelin, A. L. and Calvet, J. C. and Roujean, J. L. and Jarlan, L.
	and Los, S. O.},
  title = {Ability of the land surface model ISBA-A-gs to simulate leaf area

@ARTICLE{Habets1999,
  author = {Habets, F. and Etchevers, P. and Golaz, C. and Leblois, E. and Ledoux,
	E. and Martin, E. and Noilhan, J. and Ottle, C.},
  title = {Simulation of the water budget and the river flows of the Rhone basin},
  journal = {J. Geophys. Res.-Atmos.},
  year = {1999},
  volume = {104},
  pages = {31145-31172},
  number = {D24},
  month = dec,
  abstract = {The paper describes the implementation of a macroscale hydrological
	model in the Rhone river basin. The hydrological model is coupled
	with a soil vegetation - atmosphere transfer scheme in order to resolve
	the daily cycle of the surface energy balance and the water budget.
	The water surface routing and the water table evolution are computed
	in the hydrological model with a daily time step. First, the important
	database collected in the Rhone basin on soil, vegetation, hydrological

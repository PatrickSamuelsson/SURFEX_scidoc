	A.EOLEOLde Rosnay, P.EOLEOLLemoine, J. -M.EOLEOLGascoin, S.EOLEOLBalsamo,
	G.EOLEOLOttle, C.EOLEOLDecharme, B.EOLEOLSaux-Picart, S.EOLEOLRamillien,
	G.},
  doi = {10.1029/2009WR008856},
